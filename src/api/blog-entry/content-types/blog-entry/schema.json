{
  "kind": "collectionType",
  "collectionName": "blog_entries",
  "info": {
    "singularName": "blog-entry",
    "pluralName": "blog-entries",
    "displayName": "BlogEntry",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string",
      "required": true
    },
    "content": {
      "type": "blocks",
      "required": true
    },
    "cover_picture": {
      "type": "media",
      "multiple": false,
      "required": true,
      "allowedTypes": [
        "images"
      ]
    },
    "author": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::author.author"
    },
    "tags": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::tag.tag"
    },
    "preview_title": {
      "type": "string",
      "required": true
    },
    "preview_picture": {
      "type": "media",
      "multiple": false,
      "required": true,
      "allowedTypes": [
        "images"
      ]
    },
    "preview_description": {
      "type": "string",
      "required": true
    },
    "date": {
      "type": "datetime",
      "required": true
    },
    "related_entries": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::blog-entry.blog-entry"
    }
  }
}
